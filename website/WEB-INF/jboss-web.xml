<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>

<jboss-web>
    <display-name>ART Website</display-name>
    <context-root>omx_art</context-root>
  
    <resource-ref>
        <res-ref-name>jdbc/artdb</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <jndi-name>java:/artdb</jndi-name>
    </resource-ref>  

    <resource-ref>
        <res-ref-name>jdbc/artdb-omx</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <jndi-name>java:/artdb-omx</jndi-name>
    </resource-ref> 
    
    <resource-ref>
        <res-ref-name>jdbc/oracleStandBydb</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <jndi-name>java:/oracleStandBydb</jndi-name>
    </resource-ref> 
        
